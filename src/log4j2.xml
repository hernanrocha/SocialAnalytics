<?xml version="1.0" encoding="UTF-8"?>
<Configuration package="log4j.test"
status="WARN">
<Appenders>
    <Console name="Console" target="SYSTEM_OUT">
        <PatternLayout pattern="%-5level %logger{36} - %msg%n"/>
    </Console>
    <File name="File" filename="log4j.log" append="false">
    	<PatternLayout pattern="%d{HH:mm:ss} %-5level %logger{36} - %msg%n"></PatternLayout></File></Appenders>
<Loggers>
    <Root level="trace">
        <AppenderRef ref="Console">
        	</AppenderRef>
        <AppenderRef ref="File"></AppenderRef></Root>
</Loggers>
</Configuration>